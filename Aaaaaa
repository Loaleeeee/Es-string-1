#include <string> #include <iostream> #include <cstdlib> #include <ctime> using namespace std; #define n 100 int main() { int i, c=0, d=0; srand (time(0)); int numeri[n]; int numerin[n]; for (i=0; i<n;i++) { c=0; numeri [i]= rand() % 10+1; cout <<"numero:" <<numeri[i] <<" "; for (int i2=0; i2<i;i2++) { if (i==0) { } else if(numeri[i]==numeri[i2]) { c++; } } if (i==0) { numerin[d] = numeri[i];d++; } } cout <<" vettore numeri non ripetuti:"; for (i=0; i<d;i++) { cout << numerin[i]<<" "; } }
